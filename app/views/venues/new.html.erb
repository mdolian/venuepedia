<h1>New venue</h1>

<% form_for(@venue) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :venue_name %><br />
    <%= f.text_field :venue_name %>
  </p>
  <p>
    <%= f.label :venue_state %><br />
    <%= f.text_field :venue_state %>
  </p>
  <p>
    <%= f.label :venue_city %><br />
    <%= f.text_field :venue_city %>
  </p>
  <p>
    <%= f.label :venue_country %><br />
    <%= f.text_field :venue_country %>
  </p>
  <p>
    <%= f.label :sbd_name %><br />
    <%= f.text_field :sbd_name %>
  </p>
  <p>
    <%= f.label :phone %><br />
    <%= f.text_field :phone %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :website %><br />
    <%= f.text_field :website %>
  </p>
  <p>
    <%= f.label :contact %><br />
    <%= f.text_field :contact %>
  </p>
  <p>
    <%= f.label :whatever %><br />
    <%= f.text_field :whatever %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', venues_path %>